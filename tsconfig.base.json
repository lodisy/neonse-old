{
    "compileOnSave": false,
    "compilerOptions": {
        "rootDir": ".",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "target": "es2015",
        "module": "esnext",
        "lib": ["es2017", "dom"],
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@neonse/nest-admin-permissions": ["libs/nest/admin/permissions/src/index.ts"],
            "@neonse/nest-common-auth": ["libs/nest/common/auth/src/index.ts"],
            "@neonse/nest-common-configs": ["libs/nest/common/configs/src/index.ts"],
            "@neonse/nest-common-graphql": ["libs/nest/common/graphql/src/index.ts"],
            "@neonse/nest-common-i18n": ["libs/nest/common/i18n/src/index.ts"],
            "@neonse/nest-common-password": ["libs/nest/common/password/src/index.ts"],
            "@neonse/nest-common-prisma": ["libs/nest/common/prisma/src/index.ts"],
            "@neonse/nest-common-roles": ["libs/nest/common/roles/src/index.ts"],
            "@neonse/nest-common-sms": ["libs/nest/common/sms/src/index.ts"],
            "@neonse/nest-common-upload": ["libs/nest/common/upload/src/index.ts"],
            "@neonse/nest-common-users": ["libs/nest/common/users/src/index.ts"],
            "@neonse/nest-products": ["libs/nest/products/src/index.ts"],
            "@neonse/nest-server-products": ["libs/nest/server/products/src/index.ts"],
            "@neonse/nest-server-user": ["libs/nest/server/user/src/index.ts"],
            "@neonse/ui": ["libs/ui/src/index.ts"],
            "@neonse/ui-animations": ["libs/ui/src/animations/src/index.ts"],
            "@neonse/ui-components": ["libs/ui/src/components/src/index.ts"],
            "@neonse/ui-icons": ["libs/ui/src/icons/src/index.ts"],
            "@neonse/ui-theme": ["libs/ui/src/theme/src/index.ts"]
        }
    },
    "exclude": ["node_modules", "tmp"]
}
