{
    "version": 2,
    "projects": {
        "api": {
            "root": "apps/api",
            "sourceRoot": "apps/api/src",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/api",
                        "main": "apps/api/src/main.ts",
                        "tsConfig": "apps/api/tsconfig.app.json",
                        "assets": ["apps/api/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/api/src/environments/environment.ts",
                                    "with": "apps/api/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "api:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/api/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/api"],
                    "options": {
                        "jestConfig": "apps/api/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "store": {
            "root": "apps/store",
            "sourceRoot": "apps/store",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/next:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "root": "apps/store",
                        "outputPath": "dist/apps/store"
                    },
                    "configurations": {
                        "production": {}
                    }
                },
                "serve": {
                    "executor": "@nrwl/next:server",
                    "options": {
                        "buildTarget": "store:build",
                        "dev": true,
                        "proxyConfig": "apps/store/proxy.conf.json"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "store:build:production",
                            "dev": false
                        }
                    }
                },
                "export": {
                    "executor": "@nrwl/next:export",
                    "options": {
                        "buildTarget": "store:build:production"
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/store"],
                    "options": {
                        "jestConfig": "apps/store/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/store/**/*.{ts,tsx,js,jsx}"]
                    }
                }
            }
        },
        "store-e2e": {
            "root": "apps/store-e2e",
            "sourceRoot": "apps/store-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/store-e2e/cypress.json",
                        "tsConfig": "apps/store-e2e/tsconfig.e2e.json",
                        "devServerTarget": "store:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "store:serve:production"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/store-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "ui": {
            "root": "libs/ui",
            "sourceRoot": "libs/ui/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/ui/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/ui"],
                    "options": {
                        "jestConfig": "libs/ui/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "storybook": {
                    "executor": "@nrwl/storybook:storybook",
                    "options": {
                        "uiFramework": "@storybook/react",
                        "port": 4400,
                        "config": {
                            "configFolder": "libs/ui/.storybook"
                        }
                    },
                    "configurations": {
                        "ci": {
                            "quiet": true
                        }
                    }
                },
                "build-storybook": {
                    "executor": "@nrwl/storybook:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "uiFramework": "@storybook/react",
                        "outputPath": "dist/storybook/ui",
                        "config": {
                            "configFolder": "libs/ui/.storybook"
                        }
                    },
                    "configurations": {
                        "ci": {
                            "quiet": true
                        }
                    }
                }
            }
        },
        "ui-e2e": {
            "root": "apps/ui-e2e",
            "sourceRoot": "apps/ui-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/ui-e2e/cypress.json",
                        "tsConfig": "apps/ui-e2e/tsconfig.e2e.json",
                        "devServerTarget": "ui:storybook"
                    },
                    "configurations": {
                        "ci": {
                            "devServerTarget": "ui:storybook:ci"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/ui-e2e/**/*.{js,ts}"]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/nest"
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "babel": true
            },
            "component": {
                "export": true,
                "project": "ui",
                "style": "none",
                "pascalCaseFiles": true,
                "directory": "./components/"
            }
        },
        "@nrwl/next": {
            "application": {
                "style": "css",
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "api"
}
