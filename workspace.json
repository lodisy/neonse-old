        "defaultCollection": "@nrwl/nest"
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "babel": true
            },
            "component": {
                "export": true,
                "project": "ui",
                "style": "none",
                "pascalCaseFiles": true,
                "directory": "./components/"
